    "version": "1",
    "manifest_version": 3,
    "browser_specific_settings": {
        "gecko": {
            "id": "oujezdsky.matyas@gmail.com",
            "strict_min_version": "42.0"
        }
    },
    "icons": {
        "16": "imgs/gelbooru_bot16.png",
        "32": "imgs/gelbooru_bot32.png",
        "48": "imgs/gelbooru_bot48.png",
